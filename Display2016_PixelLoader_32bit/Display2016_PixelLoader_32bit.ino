    unsigned long startTime = millis();
    if (myFile){
        for(int i = 0; i < lightCount; i++){
          for (int j = 0; j < 12; j++){
            pixelData[i][j] = myFile.read();
            pixelData[i][j] += myFile.read() << 8;
            pixelData[i][j] += myFile.read() << 16;
            pixelData[i][j] += myFile.read() << 24;
            //Serial.print(pixelData[i][j], HEX);    //comment out for speed test
            //if (j < 11) Serial.print(", ");        //comment out for speed test
            //if (j == 11) Serial.println();         //comment out for speed test
          }
        }
    }
    myFile.close();
    unsigned long endTime = millis();
    unsigned long elapsedTime = endTime - startTime;
    Serial.print("Elapsed time for file read and reassembly = ");
    Serial.print(elapsedTime);
    Serial.println(" mS");
    while(1);
    
  
}

